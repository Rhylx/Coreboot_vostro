# layout for firmware residing at top of 4GB address space
# +-------------+ <-- 4GB - ROM_SIZE / start of flash
# | unspecified |
# +-------------+ <-- 4GB - BIOS_SIZE
# | FMAP        |
# +-------------+ <-- 4GB - BIOS_SIZE + FMAP_SIZE
# | CBFS        |
# +-------------+ <-- 4GB / end of flash

FLASH@4286578688 0x800000 {
	BIOS@7340032 1048576 {
		CONSOLE@0 0x20000
		RW_MRC_CACHE@131072 0x10000
		
		FMAP@196608 0x200
		COREBOOT(CBFS)@197120 851456
	}
}
